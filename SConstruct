import os,syscwd = os.getcwd()run = os.systemlibxxx_path = os.path.join(cwd,'..','..','xxx')include_dirs = []library_dirs = []libs = ['kernel32']envs = Environment(MSVC_VERSION='9.0',                  CCFLAGS = ['/MT','/DWIN32'],                  CPPPATH=include_dirs)                  envs.Program('armstrom',               Glob('*.c'),              LIBS = libs,               LIBPATH = library_dirs)